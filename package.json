  "jest": {
    "transform": {
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest",
      ".*\\.yaml": "<rootDir>/configs/yamlLocales.js",
      ".*": "babel-jest"
    },
    "transformIgnorePatterns": [],
    "setupFiles": [
      "<rootDir>/configs/globals.js"
    ],
    "moduleNameMapper": {
      "^vue$": "vue/dist/vue.common.js",
      "^dictionary\\/(.+)\\.yaml$": "<rootDir>/locales/$1.yaml",
